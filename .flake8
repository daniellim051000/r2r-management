[flake8]
max-line-length = 91
extend-ignore = E203, W503, I202, I100, I101
exclude = 
    .git,
    __pycache__,
    build,
    dist,
    *.egg-info,
    migrations,
    manage.py
per-file-ignores =
    # Ignore missing docstrings in tests
    tests/*:D100,D101,D102,D103
import-order-style = google
max-complexity = 10